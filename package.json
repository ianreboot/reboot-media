{
  "name": "reboot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "echo '‚ùå DEPRECATED: Use build:dev or build:prod instead' && exit 1",
    "build:dev": "cp index.dev.html index.html && tsc -b && vite build --mode development && node scripts/validate-build.js",
    "build:prod": "cp index.prod.html index.html && tsc -b && vite build --mode production && node scripts/validate-build.js",
    "validate": "node scripts/validate-build.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:dev": "./scripts/deploy-dev.sh",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "devops:deploy": "node devops/deploy-agent.js deploy",
    "devops:deploy:dev": "node devops/deploy-agent.js deploy development",
    "devops:deploy:prod": "node devops/deploy-agent.js deploy production",
    "devops:status": "node devops/deploy-agent.js status",
    "devops:rollback": "node devops/deploy-agent.js rollback",
    "monitor": "node devops/monitor.js monitor",
    "monitor:check": "node devops/monitor.js check",
    "monitor:summary": "node devops/monitor.js summary"
  },
  "dependencies": {
    "lucide-react": "^0.536.0",
    "puppeteer": "^24.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
