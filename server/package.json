{
  "name": "reboot-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Secure server for Reboot Media form processing",
  "main": "dist/simple-server.js",
  "scripts": {
    "dev": "tsx watch src/simple-server.ts",
    "build": "tsc -p tsconfig.prod.json",
    "build:prod": "tsc -p tsconfig.prod.json",
    "start": "node dist/simple-server.js",
    "start:dev": "npm run build && NODE_ENV=development npm start",
    "start:prod": "npm run build && NODE_ENV=production npm start",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
