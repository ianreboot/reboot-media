{
  "dashboard": {
    "id": null,
    "title": "Reboot Media - Executive Dashboard",
    "tags": ["executive", "business", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "Key Business Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "reboot:leads:conversion_rate_1h * 100",
            "legendFormat": "Conversion Rate %",
            "refId": "A"
          },
          {
            "expr": "sum(increase(leads_submitted_total[24h]))",
            "legendFormat": "Daily Leads",
            "refId": "B"
          },
          {
            "expr": "sum(increase(page_views_total[24h]))",
            "legendFormat": "Daily Visitors",
            "refId": "C"
          },
          {
            "expr": "reboot:availability:ratio_5m * 100",
            "legendFormat": "System Uptime %",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "green", "value": 3}
              ]
            },
            "mappings": []
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "System Uptime %"},
              "properties": [
                {"id": "min", "value": 95},
                {"id": "max", "value": 100},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 99.5},
                    {"color": "green", "value": 99.9}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Lead Generation Performance (24h)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(leads_submitted_total[1h])) * 3600",
            "legendFormat": "Leads per Hour",
            "refId": "A"
          },
          {
            "expr": "sum(rate(page_views_total{page=\"/contact\"}[1h])) * 3600",
            "legendFormat": "Contact Page Views per Hour",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "Conversion Funnel (24h)",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(increase(page_views_total[24h])) - sum(increase(page_views_total{page=\"/contact\"}[24h]))",
            "legendFormat": "General Visitors",
            "refId": "A"
          },
          {
            "expr": "sum(increase(page_views_total{page=\"/contact\"}[24h])) - sum(increase(leads_submitted_total[24h]))",
            "legendFormat": "Contact Page Visitors",
            "refId": "B"
          },
          {
            "expr": "sum(increase(leads_submitted_total[24h]))",
            "legendFormat": "Converted Leads",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 4,
        "title": "Core Web Vitals Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(web_vitals_lcp_bucket[5m])) by (le))",
            "legendFormat": "LCP P75 (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.75, sum(rate(web_vitals_cls_bucket[5m])) by (le))",
            "legendFormat": "CLS P75",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.75, sum(rate(web_vitals_fid_bucket[5m])) by (le))",
            "legendFormat": "FID P75 (ms)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2500},
                {"color": "red", "value": 4000}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "CLS P75"},
              "properties": [
                {"id": "unit", "value": "none"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 0.1},
                    {"color": "red", "value": 0.25}
                  ]
                }}
              ]
            }
          ]
        },
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 17}
      },
      {
        "id": 5,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(up{job=~\"backend.*|frontend.*\"})",
            "legendFormat": "Services Up",
            "refId": "A"
          },
          {
            "expr": "reboot:latency:p95_5m * 1000",
            "legendFormat": "Response Time P95 (ms)",
            "refId": "B"
          },
          {
            "expr": "reboot:error_rate:ratio_5m * 100",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Response Time P95 (ms)"},
              "properties": [
                {"id": "unit", "value": "ms"},
                {"id": "max", "value": 2000},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 500},
                    {"color": "red", "value": 1000}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Error Rate %"},
              "properties": [
                {"id": "max", "value": 5},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 1},
                    {"color": "red", "value": 2}
                  ]
                }}
              ]
            }
          ]
        },
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 17}
      },
      {
        "id": 6,
        "title": "Revenue Impact Metrics",
        "type": "table",
        "targets": [
          {
            "expr": "sum(increase(leads_submitted_total{source=\"organic\"}[24h]))",
            "legendFormat": "Organic Leads",
            "refId": "A",
            "instant": true
          },
          {
            "expr": "sum(increase(leads_submitted_total{source=\"paid\"}[24h]))",
            "legendFormat": "Paid Leads",
            "refId": "B",
            "instant": true
          },
          {
            "expr": "sum(increase(leads_submitted_total{source=\"referral\"}[24h]))",
            "legendFormat": "Referral Leads",
            "refId": "C",
            "instant": true
          },
          {
            "expr": "sum(increase(leads_submitted_total{source=\"direct\"}[24h]))",
            "legendFormat": "Direct Leads",
            "refId": "D",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "color-background-solid",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "color": {"mode": "thresholds"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 21}
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "increase(deployment_total[1m])",
          "iconColor": "blue",
          "titleFormat": "Deployment",
          "textFormat": "{{ deployment_version }}"
        }
      ]
    },
    "links": [],
    "version": 1
  }
}